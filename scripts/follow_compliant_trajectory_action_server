import rospy
from compliant_trajectory_control.follow_compliant_trajectory_action_server import FollowCompliantTrajectoryActionServer

def main():
    rospy.init_node('follow_compliant_trajectory_server')

    # Create the FollowCompliantTrajectoryActionServer
    follow_compliant_trajectory_server = FollowCompliantTrajectoryActionServer()

    rospy.spin()

if __name__ == '__main__':
    main()